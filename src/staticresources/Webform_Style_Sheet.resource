.bopssubmitbtn {
    background: #ffc766; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: -moz-linear-gradient(top, #ffc766 0%, #ffa100 52%, #ff8600 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffc766), color-stop(52%,#ffa100), color-stop(100%,#ff8600)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #ffc766 0%,#ffa100 52%,#ff8600 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #ffc766 0%,#ffa100 52%,#ff8600 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #ffc766 0%,#ffa100 52%,#ff8600 100%); /* IE10+ */
    background: linear-gradient(to bottom, #ffc766 0%,#ffa100 52%,#ff8600 100%); /* W3C */
    -webkit-box-shadow: 0 1px 3px rgba(0,0,0,.7);
    -moz-box-shadow: rgba(0,0,0,.7) 0 1px 3px;
    box-shadow: 0 1px 3px rgba(0,0,0,.7);
    width: auto;
    height: 30px;
    text-decoration: none;
    display: inline-block;
    outline: none;
    border: none;
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffc766', endColorstr='#ff8600',GradientType=0 ); /* IE6-8 */
    /* Font CSS */
    color: #FFF;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 14px;
    font-weight: 600;
    text-shadow: -1px -1px 0px rgba(0,0,0,.2), rgba(255,255,255,.2) 1px 1px 0;
    /*Rounded Corners*/
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    border-radius: 5px;
    margin-left: 15%;
    margin-top: 2%;
    margin-bottom: 2%;
}
.bopsrequestsubmitted {
	display:none;
	background:white; 
	
}

